    "\n",
    "guild_data['n_energy_incomes'].fillna(0, inplace=True)\n",
    "guild_data['n_unique_energy_sources'].fillna(0, inplace=True)\n",
    "\n",
    "guild_data['had_mission_reward'].fillna(False, inplace=True)\n",
    "guild_data['had_knowledge_reward'].fillna(False, inplace=True)\n",
    "guild_data['had_raid_reward'].fillna(False, inplace=True)"
   ]
