   "metadata": {},
   "outputs": [],
   "source": [
    "# проведем вычисления данных об энергии в разрезе гильдий\n",
    "energy_groupped = energy.groupby('guild_id', as_index=False).agg(\n",
    "    energy_mined_total = ('energy_delta', 'sum'),\n",
    "    n_energy_incomes = ('context_type', 'count'),\n",
    "    n_unique_energy_sources = ('context_type', 'nunique'),\n",
    "    had_mission_reward = ('context_type', lambda x: 'GuildMissionRewardContext' in x.values),\n",
    "    had_knowledge_reward = ('context_type', lambda x: 'ConvertKnowledgeToGuildEnergyContext' in x.values),\n",
    "    had_raid_reward = ('context_type', lambda x: 'CompleteGuildRaidMatchContext' in x.values),\n",
    ")"
   ]
  },
