   "source": [
    "# посчитаем долю платящих пользователей для каждого из кластеров \n",
    "stat_by_cluster['paying_share'] = round(stat_by_cluster.paying_users / stat_by_cluster.all_users * 100, 2) \n",
    "\n",
    "# посчитаем среднюю прибыль с платящего пользователя для каждого из кластеров \n",
    "stat_by_cluster['arppu'] = round(stat_by_cluster.revenue / stat_by_cluster.paying_users, 2)\n",
    "\n",
    "stat_by_cluster.sort_values(['paying_share', 'arppu'], ascending=False) \\\n",
    "               .style.background_gradient(subset=['revenue', 'paying_share', 'arppu'], cmap='PuBu')"
   ]
  },
