   "source": [
    "# сгруппируем данные по номеру недели и активности\n",
    "guild_by_week = guild_data.groupby(['week', 'is_active'], as_index=False) \\\n",
    "                          .agg({'n_added_characters':'median',\n",
    "                                'n_removed_characters':'median',\n",
    "                                'n_talents':'median',\n",
    "                                'level': 'median',\n",
    "                                'n_missions_completed': 'median',\n",
    "                                'n_times_flag_turned_on': 'median',\n",
    "                                'mean_place': 'median',\n",
    "                                'mean_rating': 'median',\n",
    "                                'n_seasons': 'median',\n",
    "                                'energy_mined_total': 'median',\n",
    "                                'n_energy_incomes': 'median',\n",
    "                                'n_unique_energy_sources': 'median'}) \\\n",
    "                         .round() \\\n",
    "                         .sort_values(['week', 'is_active'])"
   ]
